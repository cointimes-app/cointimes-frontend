{
  "short_name": "XTIMES",
  "name": "Earn money by being yourself.",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "service_worker": "static/js/background.js"
  },
  "permissions": [
    "activeTab",
    "history",
    "storage"
 ],
 "icons": {
  "16": "icons/icon16.png",
  "32": "icons/icon32.png",
  "48": "icons/icon48.png",
  "128": "icons/icon128.png"
}
}
